<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.newlandapps.fhl" >

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION." />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fhl"
        tools:targetApi="31" >
        <activity
            android:name=".MainActivityloc"
            android:exported="false"
            android:label="@string/title_activity_main_activityloc"
            android:theme="@style/Theme.Fhl.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.login"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".mainContainer.mainContainer"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".mlkit.BarcodeScannerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".allvalidations.allvalidations"
            android:screenOrientation="portrait" />
        <activity
            android:name=".locator.locator"
            android:screenOrientation="portrait" />
        <activity
            android:name=".signature.signature"
            android:screenOrientation="portrait" />
        <activity
            android:name=".photoCollection.imageGallery"
            android:exported="true"
            android:screenOrientation="portrait" >
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/keyGoogleMaps" />
    </application>

</manifest>